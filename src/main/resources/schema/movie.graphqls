type Movie implements BaseCollectable {
    id: ID!
    createdBy: String!
    createdOn: String!
    lastModifiedBy: String
    lastModifiedOn: String!
    title: String
    artwork: String
    contentRating: String
    files: [MediaFile]!
    studios: [Company]!
    cast: [Person]!
    ratings: [Rating]!
    reviews: [Review]!
}
